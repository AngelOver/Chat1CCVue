{
  "version": 2,
  "builds": [
    {
      "src": "pnpm install && pnpm build && pnpm run prod",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["build/**"],
        "excludeFiles": ["node_modules/**"]
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "build/index.js"
    }
  ]
}